{
  "name": "@bent-env/images",
  "private": true,
  "version": "1.0.0",
  "description": "bentenv.com container images",
  "scripts": {
    "docker:build": "cd $image && docker build . --rm --tag bent:$image",
    "docker:export": "cd scripts && ./export.sh $image",
    "v86:boot": "cd scripts && ./boot.js $image $v86",
    "docker:build:debian": "image=debian npm run docker:build",
    "docker:build:generic": "image=generic npm run docker:build",
    "docker:build:podman": "image=podman npm run docker:build",
    "docker:build:nodejs": "image=nodejs npm run docker:build",
    "docker:build:python": "image=python npm run docker:build",
    "docker:build:golang": "image=golang npm run docker:build",
    "docker:build:terraform": "image=terraform npm run docker:build",
    "docker:build:gstreamer": "image=gstreamer npm run docker:build",
    "docker:export:debian": "image=debian npm run docker:export",
    "docker:export:generic": "image=generic npm run docker:export",
    "docker:export:podman": "image=podman npm run docker:export",
    "docker:export:nodejs": "image=nodejs npm run docker:export",
    "docker:export:python": "image=python npm run docker:export",
    "docker:export:golang": "image=golang npm run docker:export",
    "docker:export:terraform": "image=terraform npm run docker:export",
    "docker:export:gstreamer": "image=gstreamer npm run docker:export",
    "v86:boot:debian": "image=debian npm run v86:boot",
    "v86:boot:generic": "image=generic npm run v86:boot",
    "v86:boot:podman": "image=podman npm run v86:boot",
    "v86:boot:nodejs": "image=nodejs npm run v86:boot",
    "v86:boot:python": "image=python npm run v86:boot",
    "v86:boot:golang": "image=golang npm run v86:boot",
    "v86:boot:terraform": "image=terraform npm run v86:boot",
    "v86:boot:gstreamer": "image=gstreamer npm run v86:boot",
    "build": "npm-run-all docker:build:*",
    "export": "npm-run-all docker:export:*",
    "boot": "npm-run-all v86:boot:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bent-env/images.git"
  },
  "author": "Sepehr Laal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bent-env/images/issues"
  },
  "homepage": "https://github.com/bent-env/images#readme",
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
